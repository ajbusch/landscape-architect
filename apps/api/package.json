{
  "name": "@landscape-architect/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/src/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "test:unit": "vitest run --project unit",
    "test:integration": "vitest run --project integration --passWithNoTests",
    "test:contract": "vitest run --project contract",
    "test:coverage-check": "vitest run --project unit --coverage",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@aws-sdk/client-dynamodb": "^3.990.0",
    "@aws-sdk/client-lambda": "^3.990.0",
    "@aws-sdk/client-s3": "^3.990.0",
    "@aws-sdk/client-secrets-manager": "^3.990.0",
    "@aws-sdk/lib-dynamodb": "^3.990.0",
    "@aws-sdk/s3-request-presigner": "^3.990.0",
    "@fastify/aws-lambda": "^6.4.0",
    "@fastify/cors": "^11.0.0",
    "@fastify/multipart": "^9.4.0",
    "@landscape-architect/shared": "workspace:*",
    "fastify": "^5.2.0",
    "pino": "^10.3.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.160",
    "@types/sharp": "^0.32.0",
    "@vitest/coverage-v8": "^4.0.18",
    "sharp": "^0.34.5",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  }
}
